{
  "manifest_version": 3,
  "name": "FocusFlow",
  "version": "1.0",
  "description": "Time-block tracking and productivity analytics tool",
  "permissions": ["storage", "alarms"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}